<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Vacancy" table="vacancy">
    <indexes>
      <index name="region" columns="region"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="organization" type="text" column="organization" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="industry" type="text" column="industry" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="profession" type="text" column="profession" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="creationdate" type="date" column="creationDate" nullable="false"/>
    <field name="dateposted" type="datetime" column="datePosted" nullable="false"/>
    <field name="identifier" type="text" column="identifier" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="hiringorganization" type="text" column="hiringOrganization" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="basesalary" type="float" column="baseSalary" precision="10" scale="0" nullable="false"/>
    <field name="title" type="text" column="title" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="employmenttype" type="text" column="employmentType" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="workhours" type="text" column="workHours" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="responsibilities" type="text" column="responsibilities" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="incentivecompensation" type="text" column="incentiveCompensation" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="requirements" type="text" column="requirements" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="socialprotecteds" type="text" column="socialProtecteds" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="metrostations" type="text" column="metroStations" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="source" type="text" column="source" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="workplaces" type="integer" column="workPlaces" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="additionalinfo" type="text" column="additionalInfo" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="deleted" type="text" column="deleted" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="vacurl" type="text" column="vacUrl" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="region" target-entity="Regions" fetch="LAZY">
      <join-columns>
        <join-column name="region" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
