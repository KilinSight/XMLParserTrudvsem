<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Organizations" table="organizations">
    <indexes>
      <index name="id" columns="id"/>
      <index name="region_id" columns="region_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="organizationId" type="text" column="organization_id" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="name" type="text" column="name" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="creationDate" type="date" column="creation_date" nullable="false"/>
    <field name="legalName" type="text" column="legal_name" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="inn" type="text" column="inn" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adressCode" type="text" column="adress_code" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstRateCompany" type="text" column="first_rate_company" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="businessSize" type="text" column="business_size" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="deleted" type="boolean" column="deleted" nullable="false"/>
    <many-to-one field="region" target-entity="Regions" fetch="LAZY">
      <join-columns>
        <join-column name="region_id" referenced-column-name="region_id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
